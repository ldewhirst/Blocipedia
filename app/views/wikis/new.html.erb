<h1>New Wiki Submission</h1>
  <div class="row">
    <div class="col-md-8">

<%= form_for [current_user, @wiki] do |f| %>
<div class="field">
  <%= f.label :title %>
  <%= f.text_field :title %>
</div>

<div class="field">
  <%= f.label :body %>
  <%= f.text_area :body %>
</div>

  <%= f.submit "Save" %>
<% end %>

</div>
</div>
