<h1>Edit Wiki</h1>
  <div class="row">
    <div class="col-md-4">

<%= form_for @wiki do |f| %>
<div class="field">
  <%= f.label :title %>
  <%= f.text_field :title %>
</div>

<div class="field">
  <%= f.label :body %>
  <%= f.text_area :body %>
</div>

  <%= f.submit "Save" %>
<% end %>

</div>
</div>
